{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "6.11.5"
            }
          }
        ]
      ],
      "plugins": [
        "add-module-exports",
        [
          "module-resolver",
          {
            "root": [
              "./src/functions"
            ]
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "6.11.5"
            }
          }
        ],
        "minify"
      ],
      "plugins": [
        "add-module-exports",
        [
          "module-resolver",
          {
            "root": [
              "./src/functions"
            ]
          }
        ]
      ]
    }
  }
}
